    <DriverFeatures>
      <!-- <DriverPackageFile Path="%WSKWorkspaceRoot%\DCHUDrivers" Name="Contoso.inf" >
        <FeatureID>CONTOSO_DRIVER</FeatureID>
      </DriverPackageFile> -->
    </DriverFeatures>
